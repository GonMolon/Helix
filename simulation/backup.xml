<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <name>Drone path</name>
    <description>Simulated flight of a drone using the hardware-abstract high-level drone programming language "Helix"</description>
    <Style id="path">
      <LineStyle>
        <color>7f00ffff</color>
        <width>4</width>
      </LineStyle>
      <PolyStyle>
        <color>7f00ff00</color>
      </PolyStyle>
    </Style>
    <Placemark>
      <name>Path</name>
      <description>Drone path</description>
      <styleUrl>#path</styleUrl>
      <LineString>
        <extrude>1</extrude>
        <altitudeMode>absolute</altitudeMode>
        <coordinates> 
            <-COORDINATES->
        </coordinates>
      </LineString>
    </Placemark>
  </Document>
</kml>
